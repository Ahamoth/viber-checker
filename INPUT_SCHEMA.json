{
  "title": "Viber Account Checker Input",
  "description": "Input schema for checking Viber account registration by phone numbers",
  "schemaVersion": 1,
  "type": "object",
  "properties": {
    "phoneNumbers": {
      "title": "Phone Numbers",
      "type": "string",
      "description": "CSV URL or single phone number (e.g., '+1234567890')",
      "editor": "text",
      "required": false
    },
    "maxRequests": {
      "title": "Maximum Requests",
      "type": "integer",
      "default": 100,
      "description": "Max numbers to check",
      "editor": "number",
      "required": false
    },
    "proxy": {
      "title": "Proxy Configuration",
      "type": "object",
      "description": "Proxy config (optional)",
      "editor": "json",
      "required": false
    }
  },
  "required": []
}
